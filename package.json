{
  "name": "inside",
  "version": "1.0.0",
  "description": "Node.js API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "createTables": "node data/createTables.js",
    "seedTables": "node data/seedTables.js",
    "rm:container": "docker rm -f inside-api inside-db",
    "compose:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "db:populate": "cat ./data/initial_seeding.sql | docker exec -i 'inside-db' psql -U inside -d inside"
  },
  "dependencies": {
    "argon2": "^0.41.1",
    "axios": "^1.7.7",
    "bulma": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "pg": "^8.8.0",
    "sequelize": "^6.31.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  }
}
